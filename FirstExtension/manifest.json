{
    "manifest_version": 3,
    "name": "ToborFairy",
    "description": "An extension to bring ToborFairy to your browser. He's enchanted to meet you.",
    "version": "1.0",
    "action": {
      "default_popup": "hi.html"
    },
    "oauth2": {
      "client_id": "285572556930-mfou1j1ijjlsa3uh2s0rpf44op8kghq0.apps.googleusercontent.com",
      "scopes":["openid", "https://www.googleapis.com/auth/calendar"]
    },
    "icons": {
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "js": ["content2.js"],
        "css": ["content.css"],
        "matches": [
          "https://calendar.google.com/*"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [ "smallToborFairy.png", "mediumToborFairy.png, toborFairy.png", 
                        "toborFairyBlink.gif", "smallToborBlink.gif", "textBubble1.png",
                        "Rectangle73.png", "loading.gif"
                      ],
        "matches": [ "<all_urls>" ]
      }
    ],
    "permissions": [
      "identity",
      "storage",
      "activeTab",
      "tabs"
    ],
    "key": ""
  }